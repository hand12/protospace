<%= form_for(@user) do |f| %>

　　<%= f.label :name %>
　　<%= f.file_field :name %>

　　<%= f.label :profile %>
　　<%= f.file_field :profile %>

　　<%= f.label :position %>
　　<%= f.file_field :position %>

　　<%= f.label :occupation %>
　　<%= f.file_field :occupation %>

　　<%= f.label :email %>
　　<%= f.file_field :email %>

　　<%= f.label :password %>
　　<%= f.file_field :password %>

　　<%= f.label :image %>
　  <%= image_tag @user.image.thumb.url if @user.image? %>
　　<%= f.file_field :image %>
　　<%= f.hidden_field :image_cache %>


<% end %>
