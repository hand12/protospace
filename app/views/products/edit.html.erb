<div class="proto-new">
  <%= form_for @product do |f| %>
    <div class="proto-new-title">
      <%= f.label :title, "タイトル" %>
      <%= f.text_field :title %><br>
      <%= f.label :catchcopy, "キャッチコピー" %>
      <%= f.text_field :catchcopy %><br>
      <%= f.label :concept, "コンセプト" %>
      <%= f.text_field :concept %><br>
      <%= f.fields_for :product_images do |product_image_form| %>
        <%= product_image_form.label :image, "イメージ" %>
        <%= product_image_form.file_field :image %><br>
      <% end  %>
      <%= f.submit "Create" %>
    </div>
  <% end %>
</div>
